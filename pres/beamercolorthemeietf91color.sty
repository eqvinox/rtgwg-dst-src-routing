\ProvidesPackageRCS $Header: /Users/joseph/Documents/LaTeX/beamer/base/themes/color/beamercolorthemeietf.sty,v d02a7cf4d8ae 2010/06/17 09:11:41 rivanvx $


\mode<presentation>

\definecolor{ietforange}{RGB}{255,199,224}
\definecolor{ietfblue}{RGB}{92,0,89}

\setbeamercolor{structure}{fg=ietfblue}

\setbeamercolor{palette primary}{fg=ietfblue,bg=ietforange!70}
\setbeamercolor{palette secondary}{fg=ietfblue,bg=ietforange!80}
\setbeamercolor{palette tertiary}{fg=ietfblue,bg=ietforange!90}
\setbeamercolor{palette quaternary}{fg=ietfblue,bg=ietforange}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{block title}{fg=ietfblue,bg=ietforange}
\setbeamercolor{block title alerted}{use=alerted text,fg=ietfblue,bg=alerted text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=ietfblue,bg=example text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}

\setbeamercolor{sidebar}{bg=ietforange!70}
  
\setbeamercolor{palette sidebar primary}{fg=ietfblue}
\setbeamercolor{palette sidebar secondary}{fg=ietfblue!75}
\setbeamercolor{palette sidebar tertiary}{fg=ietfblue!75}
\setbeamercolor{palette sidebar quaternary}{fg=ietfblue}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>

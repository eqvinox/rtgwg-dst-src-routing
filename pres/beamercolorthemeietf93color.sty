\ProvidesPackageRCS $Header: /Users/joseph/Documents/LaTeX/beamer/base/themes/color/beamercolorthemeietf.sty,v d02a7cf4d8ae 2010/06/17 09:11:41 rivanvx $


\mode<presentation>
\definecolor{ietfbg}{RGB}{144,231,140}
\definecolor{ietfbg2}{RGB}{166,238,162}
\definecolor{ietfbg3}{RGB}{187,245,184}
\definecolor{ietfbg4}{RGB}{209,249,207}
\definecolor{ietffg}{RGB}{0,0,0}

\setbeamercolor{structure}{fg=ietfbg!50!ietffg}

\setbeamercolor{palette primary}{fg=ietffg,bg=ietfbg4}
\setbeamercolor{palette secondary}{fg=ietffg,bg=ietfbg3}
\setbeamercolor{palette tertiary}{fg=ietffg,bg=ietfbg2}
\setbeamercolor{palette quaternary}{fg=ietffg,bg=ietfbg}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{block title}{fg=ietffg,bg=ietfbg2}
\setbeamercolor{block title alerted}{use=alerted text,fg=ietffg,bg=alerted text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=ietffg,bg=example text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}

\setbeamercolor{sidebar}{bg=ietfbg4}
  
\setbeamercolor{palette sidebar primary}{fg=ietffg}
\setbeamercolor{palette sidebar secondary}{fg=ietffg!75}
\setbeamercolor{palette sidebar tertiary}{fg=ietffg!75}
\setbeamercolor{palette sidebar quaternary}{fg=ietffg}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
